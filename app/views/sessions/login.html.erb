<h1>Log in Page </h1>
<% if @errors %>
<%= @errors %>
<% end %>

<%= form_for @user, url: 'login' do |f| %>
<div>
    <%= f.label :username %>
    <%= f.text_field :username %>
</div>    

<div>
    <%= f.label :password %>
    <%= f.password_field :password %>
</div>
    <%= f.submit "Log In"%>

    <% end %>