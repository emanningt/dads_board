<h1>Log in Page </h1>

<p><%= flash[:error] %> </p>

<%= form_for(:user) do |f| %>
<div>
    <%= f.label :email %>
    <%= f.text_field :email %>
</div>    

<div>
    <%= f.label :password %>
    <%= f.password_field :password %>
</div>
    <%= f.submit "Log In"%>

    <% end %>

    <%= link_to 'GitHub Login', '/auth/github' %>