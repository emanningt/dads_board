<%= form_for(:idea) do |f| %>

    <%= f.label :title %>
    <%= f.text_field :title %><br>    

    <%= f.label :description %>
    <%= f.text_field :description %><br>

    <p> Which type of idea does this belong to? </p>
        <%= f.collection_select :type_id, Type.all, :id, :name %>

    <p>You could create your own type if you dont see one you like.<p>
        <%= f.fields_for :type do |i| %>
            <%= i.label :name %>
            <%= i.text_field :name %>
        <% end %><br>
    <%= f.submit%>

    <% end %>

